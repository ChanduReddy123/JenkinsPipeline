pipeline{
    agent {
    label 'jenkins_agent1'
    }
    stages{
        stage('one'){
            steps{
                git(
                       url: 'https://github.com/ChanduReddy123/MySampleJava.git',
                       branch: "master"
                     )
                sh '''
                #!/bin/sh
                cd MySampleJava/SpringMVCSecurityXML/
                mvn clean package
                '''
            }
        }
        stage('two')
        {
            steps{
                input('Hey build completed would you like to continue...')
            }
        }
        stage('three')
        {
            steps{
                echo 'yoo this is done '
            }
        }
        stage('tada')
        {
            parallel{
                stage('p1')
                {
		            steps{
		                git(
		                       url: 'https://github.com/ChanduReddy123/MySampleJava.git',
		                       branch: "master"
		                     )
		                sh '''
		                #!/bin/sh
		                cd MySampleJava/SpringMVCSecurityXML/
		                mvn clean package
		                '''
		            }
                }
                stage('p2')
                {
                    steps{
                        build 'testing'
                    }
                }
            }
        }

    }
}
